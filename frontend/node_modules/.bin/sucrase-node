#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/old-arch/home/ash/git/live-polling/frontend/node_modules/sucrase/bin/node_modules:/old-arch/home/ash/git/live-polling/frontend/node_modules/sucrase/node_modules:/old-arch/home/ash/git/live-polling/frontend/node_modules:/old-arch/home/ash/git/live-polling/node_modules:/old-arch/home/ash/git/node_modules:/old-arch/home/ash/node_modules:/old-arch/home/node_modules:/old-arch/node_modules:/node_modules:/old-arch/home/ash/git/live-polling/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/old-arch/home/ash/git/live-polling/frontend/node_modules/sucrase/bin/node_modules:/old-arch/home/ash/git/live-polling/frontend/node_modules/sucrase/node_modules:/old-arch/home/ash/git/live-polling/frontend/node_modules:/old-arch/home/ash/git/live-polling/node_modules:/old-arch/home/ash/git/node_modules:/old-arch/home/ash/node_modules:/old-arch/home/node_modules:/old-arch/node_modules:/node_modules:/old-arch/home/ash/git/live-polling/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sucrase/bin/sucrase-node" "$@"
else
  exec node  "$basedir/../sucrase/bin/sucrase-node" "$@"
fi
